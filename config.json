{
  "input_path": "data/testdaten.json",
  "sensor_list": ["Accelerometer","Gyroscope","Location"], 
  "anti_aliasing_lowpass": {
    "Accelerometer": {
      "target_rate": 100,
      "order": 6,
      "include_columns": ["x", "y", "z"]
    },
    "Gyroscope": {
      "target_rate": 100,
      "order": 6,
      "include_columns": ["x", "y", "z"]
    }
  },
  "resample_imu": {
    "target_rate": 100,
    "agg_func": "mean",
    "interp_method": "time"
  },
  "resample_location": {
    "target_rate": 1,
    "fill_method": "ffill",
    "limit": null
  },
  "trim_to_common_timeframe": {
    "align_to_sensor": null,
    "warn_if_cut_seconds": 10
  },
  "hp_filters": {
    "Accelerometer": {
      "cutoff_freq": 2,
      "sample_rate": 100,
      "order": 4,
      "include_columns": ["x", "y", "z"]
    },
    "Gyroscope": {
      "cutoff_freq": 1,
      "sample_rate": 100,
      "order": 4,
      "include_columns": ["x", "y", "z"]
    }
  },
  "velocity_normalization": {
    "velocity_epsilon": 0.1,
    "v_confidence_threshold": 0.5,
    "confidence_strategy": "hard_threshold"
  },
  "window_duration_s": 4,
  "window_hop_s": 2,
  "test_key": "test_value"
}
