{
  "input_path": "data/testdaten.json",
  "sensor_list": ["Accelerometer","Gyroscope","GameOrientation","Location"],
  "resample_imu": {
    "target_rate": 100,
    "agg_func": "mean",
    "interp_method": "time"
  },
  "resample_location": {
    "target_rate": 1,
    "fill_method": "ffill",
    "limit": null
  },
  "trim_to_common_timeframe": {
    "align_to_sensor": null,
    "warn_if_cur_seconds": 10
  },
  "hp_filters": {
    "Accelerometer": {
      "cutoff_freq": 2,
      "sample_rate": 100,
      "order": 4,
      "include_columns": ["x", "y", "z"]
    },
    "Gyroscope": {
      "cutoff_freq": 1,
      "sample_rate": 100,
      "order": 4,
      "include_columns": ["x", "y", "z"]
    }
  },
  "test_key": "test_value"
}
